<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>VeganShopinGuide</name>
    <description>
      $app description
    </description>
    <author email="dev@ansertm.io" href="http://asertm.io">
     Wilferson Avila and Jonathan Barillas
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="http://veganshoppingapp.com/*" />
    <allow-navigation href="*" />
    <preference name="SplashScreenDelay" value="30000" />
    <preference name="SplashScreen" value="res/screen/all/screen-loading.png" />
    <preference name="SplashScreenBackgroundColor" value="white" />
    <preference name="ShowSplashScreen" value="true" />
    <preference name="SplashScreenWidth" value="600" />
    <preference name="SplashScreenHeight" value="300" />
</widget>
